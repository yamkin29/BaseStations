@model BaseStations.Models.BaseStation

@{
    ViewData["Title"] = "Details";
}

<h1>Подробности</h1>

<div>
    <hr />
    <dl class="row">
        <dt class = "col-sm-2">
            Номер
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Number)
        </dd>
        <dt class = "col-sm-2">
            Частота
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Frequency)
        </dd>
        <dt class = "col-sm-2">
            Координаты
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Coordinates)
        </dd>
        <dt class = "col-sm-2">
            Адрес
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Address)
        </dd>
        <dt class = "col-sm-2">
            Оператор
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Operator)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" asp-route-id="@Model?.Id">Редактировать</a> |
    <a asp-action="Index">Назад к списку</a>
</div>
